        <!-- Valve Type -->      
        <record id="trk_valve_type_tree" model="ir.ui.view">
            <field name="name">Valve Type</field>
            <field name="model">trk.valve.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="trk_valve_type_form" model="ir.ui.view">
            <field name="name">Valve Type</field>
            <field name="model">trk.valve.type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="code" class="oe_inline"/>
                            <field name="name" class="oe_inline"/>
                        </group>
                        <notebook>
                            <page string="Component">
                                <field name="component_ids"
                                    widget="section_and_note_one2many">
                                    <tree editable="bottom">
                                        <field name="code"/>
                                        <field name="name"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="trk_valve_type_action" model="ir.actions.act_window">
            <field name="name">Valve Type</field>
            <field name="res_model">trk.valve.type</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create Document
                </p>                
            </field>
        </record>

        <menuitem id="trk_valve_type_menu" 
            name="Valve Type" 
            sequence="100"
            parent="sale.product_menu_catalog"
            action="trk_valve_type_action"/>

        <!-- Menu for TRK Master -->
        <menuitem id="menu_trk_master" 
            name="Master Data" 
            sequence="10"
            parent="sale.sale_menu_root"/>

        <!-- Valve Type Component -->      
        <record id="trk_valve_type_component_tree" model="ir.ui.view">
            <field name="name">Valve Type Component</field>
            <field name="model">trk.valve.type.component</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="trk_valve_type_component_action" model="ir.actions.act_window">
            <field name="name">Valve Type Component</field>
            <field name="res_model">trk.valve.type.component</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create Document
                </p>                
